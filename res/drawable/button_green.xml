<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
      		<solid
          		android:color="#A6CB45" />
      		<stroke
          		android:width="1dp"
          		android:color="#71B238" />
      		<corners
          		android:radius="6dp" />
      		<padding
          		android:left="20dp"
          		android:top="10dp"
          		android:right="20dp"
          		android:bottom="10dp" />
    		</shape>
    </item>
    <item>
    	<shape>
      		<gradient
         		 android:startColor="#a6cb45"
          		android:endColor="#6A8347"
          		android:angle="270" />
      		<stroke
       		   android:width="1dp"
       		   android:color="#71B238" />
     		<corners
     		    android:radius="6dp" />
      		<padding
          		android:left="20dp"
          		android:top="10dp"
          		android:right="20dp"
          		android:bottom="10dp" />
    	</shape>
  </item>
</selector>